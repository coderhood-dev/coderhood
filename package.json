{
  "name": "react-three-next",
  "version": "2.0.0",
  "authors": [
    "Renaud ROHLINGER <https://twitter.com/onirenaud>"
  ],
  "private": false,
  "scripts": {
    "lint": "yarn prettier && yarn eslint",
    "eslint": "eslint --fix 'src/**/*.{js,jsx}' --ext jsconfig.json",
    "prettier": "prettier --list-different '**/*.{js,jsx,md}'",
    "dev": "next dev",
    "build": "next build",
    "export": "EXPORT=true next build && EXPORT=true next export",
    "analyze": "ANALYZE=true next build",
    "start": "next start"
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@react-three/a11y": "^2.0.2",
    "@react-three/drei": "^4.3.3",
    "@react-three/fiber": "^6.0.16",
    "esbuild-loader": "^2.12.0",
    "framer-motion": "^4.1.16",
    "next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.1.2",
    "three": "^0.128.0",
    "three-stdlib": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@next/bundle-analyzer": "^10.0.7",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-glsl": "^1.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "lint-staged": "^10.5.4",
    "next-compose-plugins": "^2.2.1",
    "next-offline": "^5.0.5",
    "postcss": "^8.2.14",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2"
  },
  "license": "MIT"
}
